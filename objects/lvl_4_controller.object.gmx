          <argument>
            <kind>1</kind>
            <string>/* DEETS 
-----------------------
-90 rows
-5 enemies
-70% obj_dirt
-30% non
    &gt;50% coins
    &gt;20% bombs
    &gt;30% empty space
-----------------------
*/

randomize();

//Initialize variables
numRows = 90;
numEnemies = 5;
blockWidth = 80;

globalvar blockSpeed;
blockSpeed = 1.2;

globalvar blocksBroken;
blocksBroken = 0;

// Message
show_message("Level 4- 900 metres");

// Generate row numbers for enemies
enemies = ds_map_create();
while (ds_map_size(enemies) &lt; numEnemies)
{
    r = irandom(numRows);
    if (is_undefined(ds_map_find_value(enemies, r)))
        ds_map_add(enemies, r, r);
}

//Generate rows
for (var i=0; i&lt; numRows; i++)
{
    if (ds_map_find_value(enemies, i))
    {
        instance_create(irandom_range(200+blockWidth/2, 840-blockWidth/2), -(blockWidth/2)-i*blockWidth+300, obj_enemy);
    }
    else
    {
        for (var n=0; n &lt; 8; n++)
        {
            if (random(100) &lt;= 70)
                instance_create(200+(blockWidth/2)+(n*blockWidth), -(blockWidth/2)-i*blockWidth+300, obj_dirt);
            else
            {
                if (choose(1,0))
                    instance_create(200+(blockWidth/2)+(n*blockWidth), -(blockWidth/2)-i*blockWidth+300, obj_coin);
                else if (choose(1,0,0,0))
                {
                    instance_create(200+(blockWidth/2)+(n*blockWidth), -(blockWidth/2)-i*blockWidth+300, obj_bomb);
                }
            }  
        }
    }
}
</string>
